<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_prefernce"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.pop.pricecutz.activities.other.PreferenceActivityFragment"
    tools:showIn="@layout/activity_preference">


    <GridView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView5"
        android:drawSelectorOnTop="true"
        android:id="@+id/fragment_preference_gidview"
        android:numColumns="2"
        android:stretchMode="columnWidth"
        android:verticalSpacing="0dp"
        android:horizontalSpacing="0dp"
        android:layout_above="@+id/fragment_preference_button" />

    <TextView
        android:text="@string/cat_pref_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView5"
        android:padding="10dp" />

    <Button
        android:text="@string/finish"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:id="@+id/fragment_preference_button"
        android:elevation="10dp"
        android:layout_alignParentBottom="true"
        android:background="@drawable/com_facebook_button_background" />

</RelativeLayout>
